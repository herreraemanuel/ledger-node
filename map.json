{
  "signup": {
    "required": ["fingerprint", "pin"]
  },
  "signin": {
    "required": ["fingerprint", "pin"]
  },
  "profile": {
    "cache": 120,
    "secure": true
  },
  "vault": {
    "required": ["title", "balance", "currency"],
    "secure": true
  },
  "transaction": {
    "required": ["category", "vault", "type", "value", "previousHash"],
    "optional": ["title", "timestamp", "location"],
    "secure": true
  },
  "transactions": {
    "optional": ["latestTransaction", "year", "month"],
    "secure": true
  },
  "place": {
    "cache": 120,
    "required": ["latitude", "longitude"],
    "secure": true
  },
  "staticmap": {
    "required": ["latitude", "longitude"],
    "optional": ["resolution", "zoom"],
    "secure": true
  },
  "status": {}
}
