{
  "signup": {
    "required": ["fingerprint", "pin"]
  },
  "signin": {
    "required": ["fingerprint", "pin"]
  },
  "profile": {
    "cache": 120,
    "secure": true
  },
  "vault": {
    "required": ["title", "balance", "currency"],
    "secure": true
  },
  "transaction": {
    "required": ["category", "vault", "type", "value", "previousHash"],
    "optional": ["title", "timestamp", "place", "latitude", "longitude"],
    "secure": true
  },
  "transactions": {
    "optional": ["latestTransaction", "year", "month"],
    "secure": true
  },
  "fork": {
    "required": ["file"],
    "secure": true
  },
  "place": {
    "cache": 120,
    "required": ["latitude", "longitude"]
  },
  "backup": {
    "required": ["key"]
  },
  "heatmap": {
    "required": ["points"],
    "optional": ["center", "color", "precission", "resolution"]
  },
  "status": {}
}
